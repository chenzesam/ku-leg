{
  "name": "rollup-kit",
  "version": "1.0.0",
  "description": "library fro scaffold",
  "main": "dist/index.common.js",
  "browser": "dist/index.browser.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "test": "jest --coverage=true",
    "prebuild": "rimraf dist && rimraf example/index.js && rimraf example/index.js.map && rimraf example/index.d.ts",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup -c ./configs/rollup.pro.js",
    "start": "cross-env NODE_ENV=development rollup --watch -c ./configs/rollup.dev.js"
  },
  "keywords": [
    "scaffold",
    "rollup"
  ],
  "author": "chenzesam",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.4",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-sizes": "^1.0.2",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.2.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-plugin-import": "^2.20.1"
  }
}
